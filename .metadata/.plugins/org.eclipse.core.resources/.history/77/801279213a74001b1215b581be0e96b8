package ex4;

public class ex04 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s= "Doi";
		switch(s) {
		case "Unu":
			System.out.print("1");
			break;
		case "Doi":
			System.out.print("2");
			break;
		case "Trei":
			System.out.print("3");
			break;
		default:
			System.out.print("Etc.");
				
		}
	}

}
