package ex1;

public class ex01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "Programare Java!";
		System.out.println("Caracterul de la pozitia 0 este " + s.charAt(0));
		System.out.println("Caracterul de la pozitia 5 este " + s.charAt(5));
		System.out.println("Ultimul caracter al sirului este " + s.charAt(s.length()));
	}

}
